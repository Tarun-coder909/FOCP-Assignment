#include <stdio.h>
int main() {
    int marks[] = {92,43,24,98,62,54,78};
    for (int i = 0; i < 7; i++) {
        if (marks[i] >= 75) {
            printf("Student %d Marks = %d\n Grade = A\n", i + 1, marks[i]);
        } else if (marks[i] >= 60) {
            printf("Student %d Marks = %d\n Grade = B\n", i + 1, marks[i]);
        } else if (marks[i] >= 40) {
            printf("Student %d Marks = %d\n Grade = C\n", i + 1, marks[i]);
        } else {
            printf("Student %d Marks = %d\n Grade = D\n", i + 1, marks[i]);
        }
    }

    return 0;
}
