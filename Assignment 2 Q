#include <stdio.h>
int main() {
    int marks[5] = {99,93,99,65,78};
    int index = -1;
    for (int i = 0; i < 5; i++) {
        if (marks[i] == 99) {
            index = i;
            break;
        }
    }
    if (index != -1) {
        printf("The first occurrence of 99 is at index: %d\n", index);
    } 
    return 0;
}
