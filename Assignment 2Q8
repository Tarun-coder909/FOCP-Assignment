#include <stdio.h>
int main() {
    int marks[5] = {95, 92, 99, 86, 77};
    int max = marks[0];
    int min = marks[0];
    
    for (int i = 0; i <5; i++) {
        if (marks[i] > max) {
            max = marks[i];
        }
        if (marks[i] < min) {
            min = marks[i];
        }
    }
    printf("Maximum marks= %d\n", max);
    printf("Minimum marks= %d\n", min);
    return 0;
}
