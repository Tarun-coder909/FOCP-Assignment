
 #include <stdio.h>
int main() {
  int a,i,sum;
  scanf("%d",&a);
  printf("divisors are\n");
  sum=0;
  for(i=1;i<a;i++){
      
    if(a%i==0){
    printf("%d\n", i);    
}
}
  sum =sum+i;
  printf("sum=%d\n",sum);
  if (sum==a) printf("it is a perfect number");
  else printf("not perfect no");
  return 0;

 }
